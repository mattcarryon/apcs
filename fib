public class fib
{
  public static int fib(int n){
    if(n < 2){
      return n;
      }
      else{
        return fib(n - 1) + fib(n - 2);
      }
  }
  public static int multiply(int x, int y){
    if(int y == 0){
      return 0;
      }
      else{
        return x + multiply(x, y-1)
        }
    }
  }
}
